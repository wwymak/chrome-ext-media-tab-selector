{
  "manifest_version": 2,

  "name": "Audio stopper",
  "description": "This extension lets you quickly navigate to the tab that has media playing",
  "version": "0.1",

  "browser_action": {
    "icons": {
      "38": "images/computer_38.png",
      "128": "images/computer_128.png" }
  },
  "background": {
    "scripts": ["js/eventPage.js"],
    "persistent": false
  },
  "permissions": [
    "tabs"
  ]
}